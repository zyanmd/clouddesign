{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/(.+)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "memory": 1024
    }
  }
}
